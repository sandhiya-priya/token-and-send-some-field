                                int CutoffFlag, CutoffFlag2, CutoffFlag3;
                                CutoffFlag = cutoff1.Checked == true ? 1 : 0;
                                CutoffFlag2 = cutoff2.Checked == true ? 1 : 0;
                                CutoffFlag3 = cutoff3.Checked == true ? 1 : 0;

                                string ChqRecFlag = "";
                                if (CutoffFlag == 1 || CutoffFlag2 == 1 || CutoffFlag3 == 1)
                                {
                                    ChqRecFlag = "N";
                                }
                                else
