                            //string sqlExistoff = "select * from IPOBidFileDetailsOffline where Exchange='BSE' and uccId='" + clcode + "' and Symbol='" + symbolhdn.Value + "' and Category='" + categoryhdn.Value + "' and Status='pending' ";
