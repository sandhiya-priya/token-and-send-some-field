                                                        qty2 = jsonmainobj.bids[1].quantity;
                                                        pr2 = jsonmainobj.bids[1].rate;
                                                        hit2 = true;

                                                        // cu2 = jsonmainobj.bids[1].cuttoffflag;                                                ;
                                                    }
