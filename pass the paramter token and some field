                                            BusinessData.NSE_ExceptionLogging.NSE_SendErrorToText("Applied the IPO in online mode for BSE: " + sqlBidD);

                                            ExceptionLogging.SendErrorToText("", "", sqlBidD);
                                            //Response.End();

